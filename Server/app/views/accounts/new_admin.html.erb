<%= form_for :account, url: accounts_path do |f|%>
<div class="well well-sm">
<%= hidden_field_tag "user_id", params[:user_id] %>
  <div class="row">
    <div class="col-md-12">
      <div class="row">
        <div class="col-md-1">
          <b>Name:<b>
        </div>
        <div class="col-md-4">
          <%=f.text_field :name %>
        </div>
      </div>
    </div>
  </div>
  <%=f.submit%>
</div>
<%end%>